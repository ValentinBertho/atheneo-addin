<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">

<Id>a1b2c3d4-5678-90ab-cdef-123456789abc</Id>
<Version>1.0.0.0</Version>
<ProviderName>Votre Entreprise</ProviderName>
<DefaultLocale>fr-FR</DefaultLocale>

<DisplayName DefaultValue="ATHENEO"/> <Description DefaultValue="Intégration avec l'ERP ATHENEO - Gestion des mails, demandes, contacts et actions"/> <IconUrl DefaultValue="https://localhost:8443/atheneo/assets/icon-32.png"/> <HighResolutionIconUrl DefaultValue="https://localhost:8443/atheneo/assets/icon-80.png"/> <SupportUrl DefaultValue="https://localhost:8443/atheneo/support"/> <AppDomains> <AppDomain>https://localhost:8443</AppDomain> <AppDomain>https://127.0.0.1:8443</AppDomain> </AppDomains> <Hosts> <Host Name="Mailbox"/> </Hosts> <Requirements> <Sets> <Set Name="Mailbox" MinVersion="1.5"/> </Sets> </Requirements> <FormSettings> <Form xsi:type="ItemRead"> <DesktopSettings> <SourceLocation DefaultValue="https://localhost:8443/atheneo/taskpane.html"/> <RequestedHeight>550</RequestedHeight> </DesktopSettings> </Form> </FormSettings>

<Permissions>ReadWriteMailbox</Permissions>

<Rule xsi:type="RuleCollection" Mode="Or"> <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/> <Rule xsi:type="ItemIs" ItemType="Message" FormType="Edit"/> </Rule> <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
<Requirements>
  <bt:Sets DefaultMinVersion="1.5">
    <bt:Set Name="Mailbox"/>
  </bt:Sets>
</Requirements>

<Hosts>
  <Host xsi:type="MailHost">
    
    <DesktopFormFactor>
      <FunctionFile resid="Commands.Url"/>
      
      <ExtensionPoint xsi:type="MessageReadCommandSurface">
        <OfficeTab id="TabDefault">
          <Group id="atheneoGroup">
            <Label resid="GroupLabel"/>
            <Control xsi:type="Button" id="atheneoTaskpaneButton">
              <Label resid="TaskpaneButton.Label"/>
              <Supertip>
                <Title resid="TaskpaneButton.Label"/>
                <Description resid="TaskpaneButton.Tooltip"/>
              </Supertip>
              <Icon>
                <bt:Image size="16" resid="Icon.16x16"/>
                <bt:Image size="32" resid="Icon.32x32"/>
                <bt:Image size="80" resid="Icon.80x80"/>
              </Icon>
              <Action xsi:type="ShowTaskpane">
                <SourceLocation resid="Taskpane.Url"/>
              </Action>
            </Control>
          </Group>
        </OfficeTab>
      </ExtensionPoint>
      
      <ExtensionPoint xsi:type="MessageComposeCommandSurface">
        <OfficeTab id="TabDefault">
          <Group id="atheneoComposeGroup">
            <Label resid="GroupLabel"/>
            <Control xsi:type="Button" id="atheneoComposeButton">
              <Label resid="TaskpaneButton.Label"/>
              <Supertip>
                <Title resid="TaskpaneButton.Label"/>
                <Description resid="ComposeButton.Tooltip"/>
              </Supertip>
              <Icon>
                <bt:Image size="16" resid="Icon.16x16"/>
                <bt:Image size="32" resid="Icon.32x32"/>
                <bt:Image size="80" resid="Icon.80x80"/>
              </Icon>
              <Action xsi:type="ShowTaskpane">
                <SourceLocation resid="Taskpane.Url"/>
              </Action>
            </Control>
          </Group>
        </OfficeTab>
      </ExtensionPoint>
    </DesktopFormFactor>
    
    <MobileFormFactor>
      <FunctionFile resid="Commands.Url"/>
      <ExtensionPoint xsi:type="MobileMessageReadCommandSurface">
        <Group id="atheneoMobileGroup">
          <Label resid="GroupLabel"/>
          <Control xsi:type="MobileButton" id="atheneoMobileButton">
            <Label resid="TaskpaneButton.Label"/>
            <Icon>
              <bt:Image size="25" resid="Icon.32x32"/>
              <bt:Image size="32" resid="Icon.32x32"/>
              <bt:Image size="48" resid="Icon.80x80"/>
            </Icon>
            <Action xsi:type="ShowTaskpane">
              <SourceLocation resid="Taskpane.Url"/>
            </Action>
          </Control>
        </Group>
      </ExtensionPoint>
    </MobileFormFactor>
    
  </Host>
</Hosts>

<Resources>
  <bt:Images>
    <bt:Image id="Icon.16x16" DefaultValue="https://localhost:8443/atheneo/assets/icon-16.png"/>
    <bt:Image id="Icon.32x32" DefaultValue="https://localhost:8443/atheneo/assets/icon-32.png"/>
    <bt:Image id="Icon.80x80" DefaultValue="https://localhost:8443/atheneo/assets/icon-80.png"/>
  </bt:Images>
  
  <bt:Urls>
    <bt:Url id="Commands.Url" DefaultValue="https://localhost:8443/atheneo/commands.html"/>
    <bt:Url id="Taskpane.Url" DefaultValue="https://localhost:8443/atheneo/taskpane.html"/>
  </bt:Urls>
  
  <bt:ShortStrings>
    <bt:String id="GroupLabel" DefaultValue="ATHENEO"/>
    <bt:String id="TaskpaneButton.Label" DefaultValue="ATHENEO"/>
  </bt:ShortStrings>
  
  <bt:LongStrings>
    <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Ouvrir le panneau ATHENEO pour enregistrer le mail, créer des demandes, consulter les contacts et gérer les actions"/>
    <bt:String id="ComposeButton.Tooltip" DefaultValue="Accéder aux fonctionnalités ATHENEO pendant la rédaction d'un mail"/>
  </bt:LongStrings>
</Resources>

</VersionOverrides> </OfficeApp>